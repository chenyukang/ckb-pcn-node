meta {
  name: Node2 open a channel to Node3
  type: http
  seq: 4
}

post {
  url: {{NODE2_RPC_URL}}/ckb
  body: json
  auth: none
}

headers {
  Content-Type: application/json
  Accept: application/json
}

body:json {
  {
    "request": {
      "OpenChannel": {
        "peer_id": "{{NODE3_PEERID}}",
        "funding_amount": 5000000
      }
    }
  }
}

assert {
  res.status: eq 200
}
